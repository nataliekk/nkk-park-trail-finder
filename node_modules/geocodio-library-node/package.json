{
  "name": "geocodio-library-node",
  "version": "1.1.0",
  "description": "geocod.io geocoding API library",
  "homepage": "https://github.com/Geocodio/geocodio-library-node",
  "author": {
    "name": "Mathias Hansen",
    "email": "mathias@geocod.io",
    "url": "https://www.geocod.io"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "geocoding",
    "geocode",
    "geo",
    "address",
    "congress",
    "timezone",
    "census"
  ],
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2"
  },
  "engines": {
    "npm": ">= 8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error",
      "camelcase": 0
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "fix": "eslint . --fix",
    "test": "jest",
    "toc": "markdown-toc README.md -i"
  },
  "repository": "Geocodio/geocodio-library-node",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0"
  }
}
